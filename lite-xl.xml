  <capabilities xmlns="http://0install.de/schema/desktop-integration/capabilities">
    <file-type id="txt" explicit-only="true">
      <extension value=".txt"/>
      <verb name="open" args="&quot;%V&quot;"/>
    </file-type>
    <file-type id="c">
      <extension value=".c"/>
      <verb name="open"><arg>${item}</arg></verb>
    </file-type>
    <file-type id="cpp">
      <extension value=".cpp"/>
      <verb name="open"><arg>${item}</arg></verb>
    </file-type>
    <file-type id="css">
      <extension value=".css"/>
      <verb name="open" args="&quot;%V&quot;"/>
    </file-type>
    <file-type id="html" explicit-only="true">
      <extension value=".html"/>
      <extension value=".htm"/>
      <verb name="open" args="&quot;%V&quot;"/>
    </file-type>
    <file-type id="js" explicit-only="true">
      <extension value=".js"/>
      <verb name="open"><arg>${item}</arg></verb>
    </file-type>
    <file-type id="lua">
      <extension value=".lua"/>
      <verb name="open"><arg>${item}</arg></verb>
    </file-type>
    <file-type id="markdown" explicit-only="true">
      <extension value=".md"/>
      <verb name="open" args="&quot;%V&quot;"/>
    </file-type>
    <file-type id="python" explicit-only="true">
      <extension value=".py"/>
      <verb name="open"><arg>${item}</arg></verb>
    </file-type>
    <file-type id="xml">
      <extension value=".xml"/>
      <verb name="open" args="&quot;%V&quot;"/>
    </file-type>
  </capabilities>
</interface>
